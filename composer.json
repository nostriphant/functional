{
    "name": "nostriphant/functional",
    "description": "Functional helper classes for nostriphant projects",
    "type": "library",
    "require": {
        "php": "^8.3"
    },
    "require-dev": {
        "pestphp/pest": "^2.35"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "nostriphant\\Functional\\": "src/",
            "nostriphant\\FunctionalTests\\": "tests/"
        }
    },
    "autoload-dev": {
        "files": ["tests/Autoload.php"]
    },
    "authors": [
        {
            "name": "Rik Meijer",
            "email": "rik@nostriphant.dev"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "preferred-install": "dist",
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "scripts": {
        "test": [
            "./vendor/bin/pest"
        ]
    }
}
